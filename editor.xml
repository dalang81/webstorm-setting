<application>
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="IS_WHEEL_FONTCHANGE_ENABLED" value="true" />
  </component>
  <component name="InlayHintsSettings">
    <option name="lastViewedProviderKeyId" value="parameter.hints.old" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern icon="important" useCustomColors="true" case-sensitive="true" pattern="\bbywukai\b*">
      <option name="FOREGROUND" value="c01614" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern icon="question" useCustomColors="true" case-sensitive="true" pattern="debug-todo">
      <option name="FOREGROUND" value="98a9c0" />
      <option name="FONT_TYPE" value="2" />
    </pattern>
    <pattern icon="question" useCustomColors="true" case-sensitive="true" pattern="\bsync local\b*">
      <option name="FOREGROUND" value="a8c023" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <filter name="mine">
      <pattern index="2" />
    </filter>
  </component>
</application>